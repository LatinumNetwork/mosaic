    "@vitejs/plugin-react": "^4.0.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chromatic": "^6.17.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "storybook": "^7.0.4",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LatinumNetwork/mosaic.git"
